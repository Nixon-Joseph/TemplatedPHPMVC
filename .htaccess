Options +FollowSymLinks
RewriteEngine on

<Files .htaccess>
    order allow,deny
    deny from all
</Files>

RewriteCond %{REQUEST_FILENAME}  !-f
RewriteCond %{REQUEST_FILENAME}  !-d

RewriteRule ^([a-zA-Z]*)\/?([a-zA-Z0-9]*)?\/?([a-zA-Z0-9\/]*)?\/?$ ./index.php?Page=$1&Action=$2&RouteParams=$3 [NC,L]